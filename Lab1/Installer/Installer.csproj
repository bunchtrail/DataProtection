<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\LicenseCore\LicenseCore.csproj" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="..\Project1\bin\Release\net8.0-windows\win-x64\publish\ProtectedApp.exe">
      <Link>ProtectedApp.exe</Link>
    </EmbeddedResource>
    <EmbeddedResource Include="..\Uninstaller\bin\Release\net8.0-windows\win-x64\publish\Uninstaller.exe">
      <Link>Uninstaller.exe</Link>
    </EmbeddedResource>
  </ItemGroup>

</Project>
